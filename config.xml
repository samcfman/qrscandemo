<?xml version='1.0' encoding='utf-8'?>
<widget id="com.qrscan.sman" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>QRCodeScan</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" /> 
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>


    <icon src="res/icon/ios/icon-57.png" platform="ios" width="57" height="57" density="mdpi" />
    <icon src="res/icon/ios/icon-72.png" platform="ios" width="72" height="72" density="mdpi" />
    <icon src="res/icon/ios/icon-29-2x.png" platform="ios" width="58" height="58" density="mdpi" />
    <icon src="res/icon/ios/icon-29.png" platform="ios" width="29" height="29" density="mdpi" />
    <icon src="res/icon/ios/icon-29-3x.png" platform="ios" width="87" height="87" density="mdpi" />
    <icon src="res/icon/ios/icon-57-2x.png" platform="ios" width="114" height="114" density="mdpi" />
    <icon src="res/icon/ios/icon-60-2x.png" platform="ios" width="120" height="120" density="mdpi" />
    <icon src="res/icon/ios/icon-60-3x.png" platform="ios" width="180" height="180" density="mdpi" />
   
</widget>
